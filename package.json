{
  "name": "server",
  "version": "1.0.0",
  "description": "The official drivesense server",
  "main": "api/app.js",
  "scripts": {
    "babel": "babel api -d dist/api -s",
    "build": "rimraf dist && npm run babel",
    "lint": "npm run lint-api && npm run lint-src && npm run lint-plop",
    "lint-api": "eslint api --fix",
    "lint-src": "echo \"build eslint-config-noamokman\"",
    "lint-plop": "eslint plop --fix",
    "api-dev": "nodemon --watch api --exec \"babel-node\" api/app.js",
    "client-dev": "babel-node webpack/webpack-dev-server.js",
    "server-dev": "concurrent \"webpack --watch --config webpack/server-dev.config.babel.js\" \"npm run server-dev-start\"",
    "server-dev-start": "just-wait -p \"dist/*.js\" && nodemon --watch dist/server.js dist/server.js",
    "dev": "concurrent \"npm run api-dev\" \"npm run client-dev\" \"npm run server-dev\" \"npm run open\"",
    "open": "opn http://localhost:9000/",
    "test": "npm run lint"
  },
  "config": {
    "ghooks": {
      "ppre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drivesense/server.git"
  },
  "keywords": [
    "drive",
    "school"
  ],
  "author": "Drivesense team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drivesense/server/issues"
  },
  "homepage": "https://github.com/drivesense/server#readme",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-class-properties": "^6.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.7.0",
    "eslint-config-noamokman": "^2.0.0",
    "file-loader": "^0.8.5",
    "ghooks": "~1.0.3",
    "isomorphic-style-loader": "0.0.12",
    "just-wait": "^1.0.5",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "nodemon": "^1.9.1",
    "opn-cli": "^3.1.0",
    "plur": "^2.1.2",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.2.0",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.9",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.1.5",
    "uppercamelcase": "^1.1.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.15",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.9.1",
    "webpack-node-externals": "^1.0.0"
  },
  "dependencies": {
    "api-error-handler": "^1.0.0",
    "axios": "^0.9.1",
    "body-parser": "^1.14.2",
    "bunyan": "^1.7.1",
    "bunyan-format": "^0.2.1",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "dotenv": "^2.0.0",
    "email-address": "^1.2.2",
    "express": "^4.13.4",
    "express-async-router": "^0.1.9",
    "express-jwt": "^3.3.0",
    "http-errors": "^1.3.1",
    "http-proxy": "^1.13.2",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^4.10.0",
    "material-ui": "^0.14.1",
    "method-override": "^2.3.5",
    "mongoose": "^4.4.12",
    "mongoose-plugin-seed": "^0.4.0",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "pify": "^2.3.0",
    "react": "^0.14.6",
    "react-cookie": "^0.4.5",
    "react-dom": "^0.14.6",
    "react-helmet": "^3.0.1",
    "react-redux": "^4.4.4",
    "react-router": "^2.1.1",
    "react-router-redux": "^4.0.2",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.4.0",
    "redux-async-connect": "^0.1.13",
    "redux-form": "^4.2.2",
    "redux-simple-promise": "^2.0.2",
    "serialize-javascript": "^1.2.0",
    "serve-favicon": "^2.3.0"
  }
}
