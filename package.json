{
  "name": "server",
  "version": "1.0.0",
  "description": "The official drivesense server",
  "main": "api/app.js",
  "scripts": {
    "babel": "babel api -d dist/api -s",
    "build": "rimraf dist && npm run babel",
    "lint": "npm run lint-api && npm run lint-src && npm run lint-plop",
    "lint-api": "eslint api --fix",
    "lint-src": "echo \"build eslint-config-noamokman\"",
    "lint-plop": "eslint plop --fix",
    "start-dev-api": "nodemon --watch api --exec \"babel-node\" api/app.js",
    "start-dev-src": "nodemon --watch src --exec \"babel-node\" src/server.universal.js",
    "start-dev-webpack": "babel-node webpack/webpack-dev-server.js",
    "start-dev": "concurrent \"npm run start-dev-api\" \"npm run start-dev-webpack\" \"npm run start-dev-src\" \"npm run open\"",
    "open": "opn http://localhost:9000/",
    "test": "npm run lint"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/drivesense/server.git"
  },
  "keywords": [
    "drive",
    "school"
  ],
  "author": "Drivesense team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/drivesense/server/issues"
  },
  "homepage": "https://github.com/drivesense/server#readme",
  "private": true,
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.0",
    "babel-eslint": "^4.1.6",
    "babel-loader": "^6.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-noamokman": "^1.0.1",
    "event-stream": "^3.3.2",
    "ghooks": "^1.0.3",
    "less": "^2.5.3",
    "less-loader": "^2.2.2",
    "material-ui": "^0.14.1",
    "nodemon": "^1.9.1",
    "open": "0.0.5",
    "opn-cli": "^3.1.0",
    "plur": "^2.1.2",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0",
    "react-router-redux": "^4.0.0",
    "react-tap-event-plugin": "^0.2.1",
    "redux": "^3.0.5",
    "redux-async-connect": "^0.1.13",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.1",
    "redux-promise-middleware": "^2.3.2",
    "redux-simple-promise": "^2.0.2",
    "redux-simple-router": "^2.0.4",
    "redux-thunk": "^2.0.1",
    "rimraf": "^2.5.2",
    "run-sequence": "^1.1.5",
    "style-loader": "^0.13.0",
    "uppercamelcase": "^1.1.0",
    "webpack": "^1.12.10",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.14.0",
    "webpack-hot-middleware": "^2.9.1"
  },
  "dependencies": {
    "api-error-handler": "^1.0.0",
    "body-parser": "^1.14.2",
    "bunyan": "^1.7.1",
    "bunyan-format": "^0.2.1",
    "compression": "^1.6.1",
    "cookie-parser": "^1.4.1",
    "dotenv": "^2.0.0",
    "email-address": "^1.2.2",
    "express": "^4.13.4",
    "express-async-router": "^0.1.9",
    "express-jwt": "^3.3.0",
    "http-errors": "^1.3.1",
    "http-proxy": "^1.13.2",
    "jsonwebtoken": "^5.7.0",
    "lodash": "^4.6.1",
    "method-override": "^2.3.5",
    "mongoose": "^4.4.6",
    "mongoose-plugin-seed": "^0.3.0",
    "morgan": "^1.7.0",
    "passport": "^0.3.2",
    "passport-facebook": "^2.1.0",
    "passport-google-oauth": "^1.0.0",
    "passport-local-mongoose": "^4.0.0",
    "pify": "^2.3.0",
    "react-helmet": "^2.3.1",
    "serialize-javascript": "^1.2.0",
    "serve-favicon": "^2.3.0",
    "webpack-isomorphic-tools": "^2.2.31"
  }
}
